      return
    #endif

    undu_get_runnumber()
    nreloadupl()
    undu_overview()

    IUNDUMAGisRunning = 0

    ifail = 1
    itry = 0
    cline = '1'
    while itry < 10:
      try:
        Fsta = open("undumag.stat","r")
        cline = Fsta.readline().strip()
        break
      except:
        itry += 1
        sleep(3)
      #endtry
    #endwhile

    if cline == '0': ifail = 0
    if ifail: wError("\n*** UNDUMAG run failed ***")

  elif Rmodus.split()[0] == 'SEGMENTATION':

    IUNDUMAGisRunning = 1
    os.system(RunUndu + " " + Rmodus)
    uwait.destroy()
    IUNDUMAGisRunning = 0

    undu_get_runnumber()
