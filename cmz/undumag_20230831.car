def update_coils():
+seq,uguiglobind.

  NCoil = len(Coils)
  DictCoils = {}
  idxcoil = 0

  for coil in Coils:
    key = coil[0][0]
    if not key in DictCoils: DictCoils[key] = []
    DictCoils[key].append(idxcoil)
    idxcoil += 1
  #endfor

