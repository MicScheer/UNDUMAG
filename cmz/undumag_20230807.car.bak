  iUndu = 0
  iEnd = 0
  iRando = 0

  NamelistVars = []
  DictNamelistVars = {}

  FileNAM = S_FileNAM.get()

  if not os.path.exists(FileNAM):
    wError("File " + FileNAM + " not found")
    return
  #endif not os.path.exists(FileNAM)

  Fnam = open(FileNAM,'r')
  LinesNam = Fnam.readlines()
  Fnam.close()

  LastNAM = FileNAM
  Fnam = open('undulast.nam','w')
  Fnam.write(LastNAM)

  nvar = -1
  iline = -1

  nomagpolmap = 0

  for line in LinesNam:

    iline += 1

    line = line.strip()
    sline = line.split('!')

    key = sline[0].strip().upper()

    if key == '$UNDUMAGN':
+self,if=debugnam.
      debug(key)
+self.
      iUndu = 1
    elif key == '$RANDOMN':
+self,if=debugnam.
      debug(key)
+self.
      iRando = 1
    elif key == '$END':
+self,if=debugnam.
      debug(key)
+self.
      iEnd += 1

    if len(line) <= 1 or line[0] == '$' or line[0] == '!': continue

    sline = line.split('!')

    if len(sline) > 1: com = sline[1].strip()
    else: com = ''

    equ = sline[0].split('=')
    if len(equ) < 2: continue

    var = equ[0].strip()
    varu = var.upper()
    val = equ[1].strip()

    if varu in DictNamelistVars:
      ivar = DictNamelistVars[varu]
      NamelistVars[ivar][1] = val
      NamelistVars[ivar][3] = iline
    else:
      nvar += 1
      DictNamelistVars[varu] = nvar
      NamelistVars.append([varu,val,com,iline])
    #endif varu in DictNamelistVars

    if varu == 'KNOMAGMAP' and int(val) != 0: nomagpolmap += 1
    if varu == 'KNOPOLMAP' and int(val) != 0: nomagpolmap += 1

  #endfor line in LinesNam

  S_MHmap = StringVar()

  if nomagpolmap != 0:
    NamelistVars[DictNamelistVars['KNOMAGMAP']][1] = '1'
    NamelistVars[DictNamelistVars['KNOPOLMAP']][1] = '1'
    S_MHmap.set('yes')
  else:
    NamelistVars[DictNamelistVars['KNOMAGMAP']][1] = '0'
    NamelistVars[DictNamelistVars['KNOPOLMAP']][1] = '0'
    S_MHmap.set('no')
  #endif

  if iUndu != 1 or iRando != 1 or iEnd != 2:
    print("iUndu, iRando, iEnd",iUndu,iRando,iEnd)
    wError("*** Bad file " + FileNAM)
