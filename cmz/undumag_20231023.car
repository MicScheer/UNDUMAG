      #endif

      scoil = ''
      for s in coil[1]:
        scoil = scoil + ' ' + s
      #endfor s in coil[1]
      Fclc.write(scoil + "\n")

    elif key == 'FILAMENTS':

      Fclc.write(coil[1] + "\n")
      for i in range(int(coil[1])):
        atom = ''
        for dat in coil[2][i]:
          atom += dat + " "
      #endfor
