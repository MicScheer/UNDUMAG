def _module_to_shift_and_rot(imodu):
  global Modules

  mo = Modules[imodu]

  lcen = mo[0]

  w = lcen.split()

  offx = calc_var(w[0])
  offy = calc_var(w[1])
  offz = calc_var(w[2])

  nper = calc_var(mo[1])
  s = mo[2].split()

  perlen = calc_var(s[0])

  vspace = [calc_var(s[1]),calc_var(s[2]),calc_var(s[3])]
  ang = calc_var(s[4])

  cen = [0.0,0.0,0.0]
  vin = [1.0,0.0,0.0]
  istat, vdum, rm = util_rotate(cen,vspace,ang,vin)

  rot11 = rm[0][0]; rot12 = rm[0][1]; rot13 = rm[0][2]
  rot21 = rm[1][0]; rot22 = rm[1][1]; rot23 = rm[1][2]
  rot31 = rm[2][0]; rot32 = rm[2][1]; rot33 = rm[2][2]

  tx = vspace[0] * perlen
  ty = vspace[1] * perlen
  tz = vspace[2] * perlen

  w = mo[3].split()

  bsx = calc_var(w[0])
  bsy = calc_var(w[1])
  bsz = calc_var(w[2])

  return nper,[tx,ty,tz],rm,ang
