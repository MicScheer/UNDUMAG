macro dpygh

* +PATCH,//UNDUMAG/SYSTEM
* +deck,dpygh, T=kumac.

exec cmzsymbols

CD python

SET python -lan
ali/cre lan python
SET cmzedit.py -E
SET $UNDUMAG_GITHUB/python/*.py -F
SET ../lib/libmypython.a -L
she cp -f ~/cmz/cmzlan.python .cmzlan

set 'chmod u+x $UNDUMAG_GITHUB/python/$compfile && python3 $compfile' -c python

lmod * last
exec lastdir python

ali/cre r 'exec rundupy' c

ali/cre eui 'edi $uguiglobals $uguiglobind $uguiini' c
ali/cre eug 'edi $unduglobal $unduglobind' c
ali/cre eugg 'edi $uguiglobals $uguiglobind' c
ali/cre emh 'edi //waves/python/$m_hbook' c
ali/cre egui 'edi $ugui' c
ali/cre emain 'edi undugui' c

she echo && echo UNDUMAG_GITHUB: $UNDUMAG_GITHUB
EXITM
